<% if current_user %>
Logged in as <%= current_user.email %> | 
	<%= link_to 'View my Profile', user_path(current_user) %> | 
  <%= link_to "Edit Profile", edit_user_path(current_user) %> |
  <%= link_to "Logout", :logout, method: :post %>
<% else %>
  <%= link_to "Register", new_user_path %> |
  <%= link_to "Login", :login %>
<% end %>