<h1>Listing checks</h1>

<table align="center">
  <thead>
    <tr>
      <th>User</th>
      <th>Restaurant</th>
      <th>Paid at</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @checks.each do |check| %>
      <tr>
        <td><%= check.user_id %></td>
        <td><%= check.restaurant_id %></td>
        <td><%= check.paid_at %></td>
        <td><%= link_to 'Show', check %></td>
        <td><%= link_to 'Edit', edit_check_path(check) %></td>
        <td><%= link_to 'Destroy', check, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

